{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "keyVaultName": {
      "value": "your-keyvault-name"
    },
    "location": {
      "value": "swedencentral"
    },   
    "tags": {
      "value": {
        "environment": "production",
        "department": "IT"
      }
    },
    "enablePurgeProtection": {
      "value": true
    },
    "enabledForDeployment": {
      "value": false
    },
    "enabledForDiskEncryption": {
      "value": false
    },
    "enabledForTemplateDeployment": {
      "value": false
    },
    "enableRbacAuthorization": {
      "value": true
    },
    "tenantId": {
      "value": "your-tenant-id"
    },
    "objectId": {
      "value": "your-object-id"
    },
    "keysPermissions": {
      "value": []
    },
    "secretsPermissions": {
      "value": []
    },
    "skuName": {
      "value": "standard"
    },
    "networkAcls": {
      "value": {
        "bypass": "None",
        "defaultAction": "Deny",
        "ipRules": [],
        "virtualNetworkRules": []
      }
    }
  }
}
